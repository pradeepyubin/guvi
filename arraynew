#include <stdio.h>
 
int main()
{
  int array[100], n, c, d, swap;
 
  printf("Enter number of elements\n");
  scanf("%d", &n);
 
  printf("Enter %d integers\n", n);
 
  for (c = 0; c < n; c++)
    scanf("%d", &array[c]);
	
	 for ( c = 0 ; c < n ; c++ )
     {
	 if(array[c]==c)
	 printf("\n %d",array[c]);
	 }
	 
	 return 0;
	 
}
